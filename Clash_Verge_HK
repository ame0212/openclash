const defaultDNS = ["tls://1.12.12.12", "tls://223.5.5.5"];
const chinaDNS = ["119.29.29.29", "180.184.1.1"];
const foreignDNS = ["tls://8.8.8.8", "tls://1.1.1.1", "tls://9.9.9.9"];

